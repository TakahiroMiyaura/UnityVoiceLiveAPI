<Project Sdk="Microsoft.NET.Sdk">
  <!--
    VoiceLiveAPI.Unity - Unity components for Azure AI VoiceLive API

    Prerequisites:
      Unity path is automatically detected via Directory.Build.props using one of:
      1. UnityPath.props (generated by scripts/Setup-UnityPath.ps1) - fastest
      2. Unity-generated csproj (requires Unity Editor to have been opened once)

    Setup:
      Option A: Open Unity project with Unity Editor (recommended for first time)
      Option B: Run 'pwsh scripts/Setup-UnityPath.ps1'

    Build:
      dotnet build
  -->

  <PropertyGroup>
    <TargetFramework>netstandard2.1</TargetFramework>
    <LangVersion>9.0</LangVersion>
    <RootNamespace>Com.Reseul.Azure.AI.VoiceLiveAPI.Unity</RootNamespace>
    <AssemblyName>Com.Reseul.Azure.AI.VoiceLiveAPI.Unity</AssemblyName>
    <GenerateDocumentationFile>False</GenerateDocumentationFile>
    <Nullable>disable</Nullable>
    <ImplicitUsings>disable</ImplicitUsings>

    <!-- NuGet Package Metadata -->
    <PackageId>Com.Reseul.Azure.AI.VoiceLiveAPI.Unity</PackageId>
    <Version>1.0.0</Version>
    <Authors>Takahiro Miyaura</Authors>
    <Company>Reseul</Company>
    <Description>Unity components for Azure AI VoiceLive API integration</Description>
    <RepositoryUrl>https://github.com/TakahiroMiyaura/VoiceLiveAPI-Unity-Local</RepositoryUrl>
    <ProduceReferenceAssembly>True</ProduceReferenceAssembly>
    <PackageLicenseFile>LICENSE</PackageLicenseFile>

    <!-- Disable default compile items - we use explicit links to Unity source -->
    <EnableDefaultCompileItems>false</EnableDefaultCompileItems>
  </PropertyGroup>

  <!-- Source files linked from Unity project -->
  <ItemGroup>
    <Compile Include="..\..\Unity\UnityVoiceLiveAPI\Assets\Reseul\UnityVoiceLiveAPI\Runtime\**\*.cs" Link="Runtime\%(RecursiveDir)%(Filename)%(Extension)" />
    <Compile Include="..\..\Unity\UnityVoiceLiveAPI\Assets\Reseul\UnityVoiceLiveAPI\Editor\**\*.cs" Link="Editor\%(RecursiveDir)%(Filename)%(Extension)" />
    <Compile Include="..\..\Unity\UnityVoiceLiveAPI\Assets\Reseul\UnityVoiceLiveAPI\Samples\**\*.cs" Link="Samples\%(RecursiveDir)%(Filename)%(Extension)" />
    <!-- Keep local Properties folder for resources -->
    <Compile Include="Properties\**\*.cs" />
  </ItemGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|AnyCPU'">
    <IsPublishable>True</IsPublishable>
    <GenerateAssemblyInfo>True</GenerateAssemblyInfo>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Release|AnyCPU'">
    <IsPublishable>True</IsPublishable>
    <GenerateAssemblyInfo>True</GenerateAssemblyInfo>
  </PropertyGroup>

  <ItemGroup>
    <Compile Remove="Runtime\Assmblies\**" />
    <EmbeddedResource Remove="Runtime\Assmblies\**" />
    <None Remove="Runtime\Assmblies\**" />
  </ItemGroup>

